<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\bflush\b.*" />
    <pattern pattern="\bfix\b.*" />
    <pattern pattern="\benhancement\b.*" />
    <pattern useCustomColors="true" pattern="urgent">
      <option name="FOREGROUND" value="bf5c64" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssColorPreviewInGutter" value="false" />
  </component>
</application>